{
  "presets": [
    "@babel/typescript",
    "@babel/react",
    ["@babel/env", {
      "targets": {
        "chrome": "64"
      },
      "useBuiltIns": "usage",
      "debug": true
    }]
  ],
  "plugins": [
    "@babel/proposal-class-properties",
    "@babel/proposal-object-rest-spread",
    "@babel/proposal-async-generator-functions",
    "@babel/proposal-optional-chaining",
    ["@babel/plugin-proposal-decorators", { "legacy": true }]
  ]
}